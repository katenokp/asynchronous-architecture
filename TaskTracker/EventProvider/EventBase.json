{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "EventBase",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "EventInfo": {
      "$ref": "#/definitions/EventInfo"
    },
    "Data": {}
  },
  "definitions": {
    "EventInfo": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "EventId",
        "Version",
        "Name",
        "Time",
        "Producer"
      ],
      "properties": {
        "EventId": {
          "type": "string",
          "format": "guid",
          "minLength": 1
        },
        "Version": {
          "type": "integer",
          "format": "int32"
        },
        "Name": {
          "type": "string",
          "minLength": 1
        },
        "Time": {
          "type": "string",
          "format": "date-time",
          "minLength": 1
        },
        "Producer": {
          "type": "string",
          "minLength": 1
        }
      }
    }
  }
}