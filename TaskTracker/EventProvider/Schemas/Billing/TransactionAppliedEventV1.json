          "minLength": 1
        }
      }
    },
    "TransactionAppliedDataV1": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "BillingCyclePublicId",
        "Description",
        "Type",
        "Debit",
        "Credit",
        "PublicId"
      ],
      "properties": {
        "BillingCyclePublicId": {
          "type": "string",
          "format": "guid",
          "minLength": 1
        },
        "Description": {
          "type": "string",
          "minLength": 1
        },
        "Type": {
          "$ref": "#/definitions/TransactionType"
        },
        "Debit": {
          "type": "number",
          "format": "decimal"
